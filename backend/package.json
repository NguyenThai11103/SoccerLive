{
  "name": "soccerlive-backend",
  "version": "1.0.0",
  "description": "SoccerLive Backend - Live Football Streaming Platform",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "nodemon --exec babel-node index.js",
    "dev": "nodemon --exec babel-node index.js",
    "make:controller": "node src/scripts/makeController.js",
    "make:route": "node src/scripts/makeRoute.js",
    "make:seeder": "node src/scripts/makeSeeder.js",
    "db:init": "npx sequelize-cli init",
    "db:seed": "npx sequelize-cli db:seed:all",
    "db:seed:one": "npx sequelize-cli db:seed --seed",
    "db:seed:undo": "npx sequelize-cli db:seed:undo:all",
    "db:seed:undo:one": "npx sequelize-cli db:seed:undo --seed"
  },
  "keywords": [
    "football",
    "streaming",
    "live",
    "soccer",
    "express",
    "mysql"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "express-validator": "^7.2.1",
    "jsonwebtoken": "^9.0.2",
    "moment": "^2.30.1",
    "multer": "^1.4.5-lts.2",
    "mysql2": "^3.12.0",
    "redis": "^4.7.0",
    "sequelize": "^6.37.7",
    "sequelize-cli": "^6.6.2",
    "socket.io": "^4.8.1",
    "uuid": "^11.1.0",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.26.4",
    "@babel/core": "^7.26.9",
    "@babel/node": "^7.26.0",
    "@babel/plugin-proposal-export-default-from": "^7.25.9",
    "@babel/plugin-transform-modules-commonjs": "^7.26.3",
    "@babel/preset-env": "^7.26.9",
    "babel-plugin-module-resolver": "^5.0.2",
    "nodemon": "^3.1.9"
  }
}
